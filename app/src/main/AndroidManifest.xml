<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.haochang.party">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogoActivity" android:exported="true">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".gles.MainOpenGLESActivity" />
        <activity android:name=".base.MainMVPActivity" />
        <activity android:name=".image.MainImageActivity" />
        <activity android:name=".network.MainNetworkActivity" />
        <activity android:name=".permission.MainPermissionActivity" />
        <activity android:name=".utils.MainUtilsActivity" />
        <activity android:name=".view.MainViewsActivity" />
        <activity android:name=".view.text.scrolltext.ScrollTextViewActivity" />
        <activity android:name=".view.scroll.ScrollTwoPageActivity" />
        <activity android:name=".gles.learn.LearnOpenGLESActivity" />
        <activity android:name=".audio.MainAudioMVPActivity" />
        <activity android:name=".video.MainVideoMVPActivity" />
    </application>
</manifest>